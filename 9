using System;

namespace BoxVolume
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Enter the length of the box: ");
            double length = double.Parse(Console.ReadLine());

            Console.Write("Enter the width of the box: ");
            double width = double.Parse(Console.ReadLine());

            Console.Write("Enter the height of the box: ");
            double height = double.Parse(Console.ReadLine());

            double volume = GetVolume(length, width, height);
            Console.WriteLine("The volume of the box is {0}.", volume);
        }

        static double GetVolume(double length, double width, double height)
        {
            return length * width * height;
        }
    }
}